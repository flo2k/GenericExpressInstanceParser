<root>
<!-- Exmaples how EXPRESS Instances are converted to XML -->


<!-- Entity() -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Entity>
  </Entity>
</geip:entities>

<!-- Entity ( ) Entity() -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Entity>
  </Entity>
  <Entity>
  </Entity>
</geip:entities>

<!-- EnumType(type := USER) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <EnumType>
    <type geip:type="enum">USER</type>
  </EnumType>
</geip:entities>

<!-- EnumType(type1 := USER, type2 := SYSTEM) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <EnumType>
    <type1 geip:type="enum">USER</type1>
    <type2 geip:type="enum">SYSTEM</type2>
  </EnumType>
</geip:entities>

<!-- EnumType(type1 := USER, type2 := Entity()) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <EnumType>
    <type1 geip:type="enum">USER</type1>
    <type2 geip:type="entity">
      <Entity></Entity>
    </type2>
  </EnumType>
</geip:entities>

<!-- ListType(emptyList := []) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <ListType>
    <emptyList geip:type="list"></emptyList>
  </ListType>
</geip:entities>

<!-- ListType(elements:= [Entity1(), Entity2()]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <ListType>
    <elements geip:type="list">
      <Entity1></Entity1>
      <Entity1></Entity1>
    </elements>
  </ListType>
</geip:entities>

<!-- Error (number:=24973) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="int">24973</number>
  </Error>
</geip:entities>

<!-- Error (number:=+24973) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="int">+24973</number>
  </Error>
</geip:entities>

<!-- Error (number:=-24973) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="int">-24973</number>
  </Error>
</geip:entities>

<!-- Error (number:=24973.44) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="real">24973.44</number>
  </Error>
</geip:entities>

<!-- Error (number:=-123.45) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="real">-123.45</number>
  </Error>
</geip:entities>

<!-- Error (number:=+123.45) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Error>
    <number geip:type="real">+123.45</number>
  </Error>
</geip:entities>

<!-- Min (min:=3.45) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Min>
    <min geip:type="real">3.45</min>
  </Min>
</geip:entities>

<!-- StringType(type:="USER") -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <StringType>
    <type geip:type="string">"USER"</type>
  </StringType>
</geip:entities>

<!-- Pair(key:="591", value:="1900") -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <Pair>
    <key geip:type="string">"591"</key>
    <value geip:type="string">"591"</value>
  </Pair>
</geip:entities>

<!-- EmptyList (elements:= []) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <EmptyList>
    <elements geip:type="list">
    </elements>
  </EmptyList>
</geip:entities>

<!-- StringList (elements:= ["element 1","element 2"]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <StringList>
    <elements geip:type="list">
      <geip:entry geip:type="string"><![CDATA["element 1"]]></geip:entry>
      <geip:entry geip:type="string"><![CDATA["element 2"]]></geip:entry>
    </elements>
  </StringList>
</geip:entities>

<!-- IntList (elements:= [1, 2, 3]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <IntList>
    <elements geip:type="list">
      <geip:entry geip:type="int">1</geip:entry>
      <geip:entry geip:type="int">2</geip:entry>
      <geip:entry geip:type="int">3</geip:entry>
    </elements>
  </IntList>
</geip:entities>

<!-- RealList (elements:= [1.1, 2.2, 3.3]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <RealList>
    <elements geip:type="list">
      <geip:entry geip:type="real">1.1</geip:entry>
      <geip:entry geip:type="real">2.2</geip:entry>
      <geip:entry geip:type="real">3.3</geip:entry>
    </elements>
  </RealList>
</geip:entities>

<!-- BoolList (elements:= [TRUE, FALSE]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <RealList>
    <elements geip:type="list">
      <geip:entry geip:type="bool">TRUE</geip:entry>
      <geip:entry geip:type="bool">FALSE</geip:entry>
    </elements>
  </RealList>
</geip:entities>

<!-- EnumList (elements:= [TYPE1, TYPE2, TYPE3]) -->
<geip:entities xmlns:geip="https://github.com/flo2k/GenericExpressInstanceParser/">
  <EnumList>
    <elements geip:type="list">
      <geip:entry geip:type="enum">TYPE1</geip:entry>
      <geip:entry geip:type="enum">TYPE2</geip:entry>
      <geip:entry geip:type="enum">TYPE3</geip:entry>
    </elements>
  </EnumList>
</geip:entities>
</root>
